" gvimrc
" Settings for VIM gui version
set antialias
" Font: git://github.com/pdf/ubuntu-mono-powerline-ttf.git
set guifont=Ubuntu\ Mono\ 16

set guioptions-=T
set guioptions-=m
set guioptions-=e
set guioptions-=L
set guioptions-=r

colorscheme solarized
set cursorline

set lines=28
set columns=100
